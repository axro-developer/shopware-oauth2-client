{
  "name": "axro/shopware-oauth2-client",
  "description": "Shopware 6 OAuth2 API Client",
  "version": "2.0.5",
  "license": "proprietary",
  "authors": [
    {
      "name": "Emre Eromay",
      "email": "eeromay@axro.de"
    }
  ],
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": ">=7.8",
    "psr/log": "^3.0 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "AxroShopware\\": "./src/"
    }
  },
  "require-dev": {
    "phpro/grumphp": "^2.4",
    "squizlabs/php_codesniffer": "^3.8",
    "enlightn/security-checker": "^2.0",
    "phpstan/phpstan": "^1.10"
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": false
    }
  }
}
